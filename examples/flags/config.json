{
  "config_uri": "file://./config.json",
  "app_name": "MyApp",
  "version": "1.0.0",
  "log_level": "info",
  "server": {
    "host": "localhost",
    "port": 8080,
    "debug": true
  },
  "database": {
    "url": "postgresql://localhost:5432/myapp",
    "max_connections": 10,
    "timeout": 30
  },
  "features": {
    "enable_cache": true,
    "enable_metrics": false,
    "allowed_origins": ["*"]
  }
}
